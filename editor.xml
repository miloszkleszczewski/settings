<application>
  <component name="CodeInsightSettings">
    <option name="PARAMETER_INFO_DELAY" value="500" />
    <option name="AUTO_POPUP_JAVADOC_INFO" value="true" />
    <option name="JAVADOC_INFO_DELAY" value="500" />
    <option name="AUTOCOMPLETE_ON_CODE_COMPLETION" value="false" />
    <option name="HIGHLIGHT_SCOPE" value="true" />
    <option name="ADD_UNAMBIGIOUS_IMPORTS_ON_THE_FLY" value="true" />
  </component>
  <component name="CodeVisionSettings">
    <option name="codeVisionGroupToPosition">
      <map>
        <entry key="inheritors" value="Default" />
        <entry key="problems" value="Default" />
        <entry key="references" value="Default" />
        <entry key="vcs.code.vision" value="Default" />
      </map>
    </option>
    <option name="disabledCodeVisionProviderIds">
      <set>
        <option value="vcs.code.vision" />
      </set>
    </option>
  </component>
  <component name="DaemonCodeAnalyzerSettings">
    <option name="AUTOREPARSE_DELAY" value="100" />
  </component>
  <component name="EditorSettings">
    <option name="USE_SOFT_WRAPS" value="CONSOLE" />
    <option name="SHOW_INSPECTION_WIDGET" value="false" />
    <option name="SHOW_INTENTION_BULB" value="false" />
    <option name="IS_ALL_SOFTWRAPS_SHOWN" value="true" />
    <option name="IS_ANIMATED_SCROLLING" value="false" />
    <option name="IS_CAMEL_WORDS" value="true" />
  </component>
  <component name="InlayHintsSettings">
    <option name="lastViewedProviderKeyId" value=".vcs.code.vision" />
  </component>
  <component name="JetEditorOptions">
    <option name="donTShowConversionDialog" value="true" />
  </component>
  <component name="TodoConfiguration">
    <pattern pattern="\btodo\b.*" />
    <pattern pattern="\bfixme\b.*" />
    <pattern icon="important" useCustomColors="true" pattern="\bblocked_by\b.*">
      <option name="FOREGROUND" value="e31000" />
      <option name="FONT_TYPE" value="2" />
      <option name="ERROR_STRIPE_COLOR" value="977ab" />
    </pattern>
    <pattern useCustomColors="true" pattern="\brefactor\b.*">
      <option name="FOREGROUND" value="a3ff" />
      <option name="FONT_TYPE" value="2" />
      <option name="ERROR_STRIPE_COLOR" value="977ab" />
    </pattern>
    <pattern useCustomColors="true" pattern="\bdrill\b.*">
      <option name="FOREGROUND" value="eab800" />
      <option name="FONT_TYPE" value="2" />
      <option name="ERROR_STRIPE_COLOR" value="977ab" />
    </pattern>
    <filter name="blocked">
      <pattern index="2" />
    </filter>
    <filter name="refactor">
      <pattern index="3" />
    </filter>
  </component>
  <component name="XmlEditorOptions">
    <option name="selectWholeCssIdentifierOnDoubleClick" value="false" />
  </component>
</application>